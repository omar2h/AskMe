#include "database_manager.hpp"

bool DbManager::check_user_has_q(int qId, int uId){
	std::pair<Question, bool> qPair = questionsDb.get_question(qId);
	if(!qPair.second)
		return 0;
	if(qPair.first.toId != uId)
		return 0;
	return 1;
}
